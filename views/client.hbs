<a href="/auth"><h1>[ HealthApp &#9829; ]</h1></a><p>

<h1>{{client.first}} {{client.last}}</h1>

<div class="client-info">

    <h2>First Name</h2>

    <p>{{client.first}}</p>

    <h2>Last Name</h2>

    <p>{{client.last}}</p>

    <h2>Sex</h2>
<div class="sexinfo">
    {{#if client.sex}}
        <p>{{client.first}} is female</p>
    {{else}}
        <p>{{client.first}} is male</p>
    {{/if}}
</div>
    <h2>Height (Feet) </h2>

    <p>{{client.height}}</p>

    <h2>Weight</h2>

    <p>{{client.weight}}</p>

    <h2>Heart Rate</h2>

    <p>{{client.heart}}</p>

    <h2>Notes</h2>

    <p>{{client.notes}}</p>


</div>

<div class="dates-update">

    <h2>Dates Updated</h2>

</div>

<p><a href="/auth"><input class="home_button" type="submit" value="H  O  M  E  "></a>

    <a href="/auth/logout"><input class="logout_button" type="submit" value="LOGOUT"></a>

<p><a href="/tasks/task"><input class="todo_button" type="submit" value="TO DO TASKS"></a>

    <a href="/tasks/tasks_completed/tasklist"><input class="completed_button" type="submit" value="COMPLETED TASKS"></a>


<script src="/javascripts/check.js"></script>  <!-- Javascript file for message box -->